{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "typescript.suggest.autoImports": true,
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}